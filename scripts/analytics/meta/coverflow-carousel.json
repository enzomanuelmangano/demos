    "input_range_calculation": {
      "pattern": "Per-item input range based on index and width",
      "formula": [
        "(index - 3) * itemWidth",
        "(index - 2) * itemWidth",
        "(index - 1) * itemWidth",
        "index * itemWidth",
        "(index + 1) * itemWidth",
        "(index + 2) * itemWidth",
        "(index + 3) * itemWidth"
